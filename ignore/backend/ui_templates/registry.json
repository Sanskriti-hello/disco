{
    "templates": {
        "code-1": {
            "id": "code-1",
            "name": "Terminal Code View",
            "component_path": "code-1/src/pages/Frame22.jsx",
            "app_path": "code-1/src/App.jsx",
            "config_path": "code-1/tailwind.config.js",
            "assets_path": "code-1/assets/",
            "legacy_css_path": "code-1-legacy.css",
            "theme": {
                "primary_color": "#00FF41",
                "secondary_color": "#003B00",
                "background_color": "#0D1117",
                "accent_color": "#39FF14",
                "style": "pixelated_terminal",
                "fonts": [
                    "JetBrains Mono",
                    "Fira Code",
                    "monospace"
                ]
            },
            "features": [
                "large_code_area",
                "syntax_highlighting",
                "terminal_output",
                "explanation_panel",
                "line_numbers",
                "file_tree"
            ],
            "data_schema": "CodeTemplateData",
            "required_fields": [
                "code_snippets"
            ],
            "optional_fields": [
                "documentation",
                "terminal_output",
                "related_repos"
            ],
            "matching": {
                "keywords": [
                    "code",
                    "programming",
                    "debug",
                    "error",
                    "function",
                    "api",
                    "github",
                    "stackoverflow",
                    "syntax",
                    "terminal",
                    "developer",
                    "coding",
                    "software",
                    "script",
                    "repository"
                ],
                "domains": [
                    "code"
                ],
                "fallback_domains": [
                    "study"
                ],
                "url_patterns": [
                    "*github.com*",
                    "*stackoverflow.com*",
                    "*developer.mozilla.org*",
                    "*docs.python.org*",
                    "*dev.to*"
                ],
                "priority": 10
            }
        },
